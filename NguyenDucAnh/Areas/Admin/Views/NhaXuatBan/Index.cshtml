@model IEnumerable<NguyenDucAnh.Models.NHAXUATBAN>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        <br />
        <a href="~/Admin/NhaXuatBan/Create">Tạo mới</a>|
    </p>
    <table class="table" border="1" cellspacing="0">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MaNXB)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TenNXB)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DiaChi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DienThoai)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MaNXB)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TenNXB)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DiaChi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DienThoai)
            </td>
            <td>
                <a href="~/Admin/NhaXuatBan/Edit?id=@item.MaNXB">Chỉnh sửa</a>|
                <a href="~/Admin/NhaXuatBan/Details?id=@item.MaNXB">Xem chi tiết</a>|

                
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
