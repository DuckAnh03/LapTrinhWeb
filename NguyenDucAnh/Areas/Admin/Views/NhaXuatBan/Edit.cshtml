@model NguyenDucAnh.Models.NHAXUATBAN

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Edit</title>
</head>
<body>

    
    <form name="NhaXuatBan" action="~/Admin/NhaXuatBan/Edit" method="post">
        <h3>Thêm mới NXB</h3>
        <table>
            <tr>
                <td><input type="hidden" name="MaNXB" value="@Model.MaNXB" /></td>
            </tr>
            <tr>
                <td>Tên NXB</td>
                <td><input type="text" name="TenNXB" value="@Model.TenNXB" /></td>
            </tr>
            <tr>
                <td>Địa chỉ</td>
                <td><input type="text" name="DiaChi" value="@Model.DiaChi" /></td>
            </tr>
            <tr>
                <td>Điện thoại</td>
                <td><input type="text" name="DienThoai" value="@Model.DienThoai" /></td>
            </tr>
            <tr>
                <td></td>
                <td>
                    <input type="submit" value="Lưu" />
                    <input type="reset" value="Reset" />
                </td>

            </tr>
        </table>
    </form>
</body>
</html>
